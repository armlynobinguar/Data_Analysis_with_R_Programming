dc_listings <- dc_listings %>%
    select(-room_type, -city, -state, 
           -latitude, -longitude, -zipcode, 
           -host_response_rate, -host_acceptance_rate, -host_listings_count)
dc_columns <- colnames(dc_listings)

dc_listings <- dc_listings %>%
  filter(
    !is.na(bedrooms),
    !is.na(bathrooms),
    !is.na(beds)
    )
